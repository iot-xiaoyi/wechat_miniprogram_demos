.badge{
  position: absolute;
  top: 18px;
  right: 40px;
  width: 15px;
  height: 15px;
  line-height: 15px;
  background: #ff0000;
  color: #fff;
  border-radius: 50%;
  text-align: center;
  font-size: 8px;
}
.with_arrow{
  position: absolute;
  top: 18px;
  right: 15px;
  width: 15px;
  height: 15px;
  /* background-image: url(../../images/icon-arrowdown.png); */
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.show-btn {
  margin-top: 100rpx;
  color: #22cc22;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 540rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -180rpx 105rpx;
  border-radius: 36rpx;
}

.pick{
  width: 350rpx; 
  height: 300rpx;
  margin-left: 200rpx;
}
.week_checkbox{
  flex-direction: column;
  align-items: center;
}
.box_normal{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 25rpx;
  border-bottom: 2rpx solid #dadada;
}
.box_normal_top{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
