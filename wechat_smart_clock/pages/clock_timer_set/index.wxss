  width: 100rpx;
  height: 100rpx;
}
.weui_cell{
    position: relative;
    display: flex;
    padding: 25rpx;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    border-bottom: 2rpx solid #dadada;
}
.weui_cell_top{
    position: relative;
    display: flex;
    padding: 5rpx;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    border-top: 2rpx solid #dadada;
}
