  border-top: 2rpx solid #dadada;
}
.box_normal_name{
  margin-right: 30rpx;
}
.box{
  justify-content: space-between;
  display: flex;
  flex-direction: row;
}
.box_value{
  margin-left: 20rpx;
  font-size: 13px;
  color: #737373;
}
.box_name{
  margin-right: 40rpx;
}
